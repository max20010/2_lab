#include "stdio.h"
int main()
{
	int x[3];
	printf("Введите три числа: \n");
	for (int i = 0; i < 3; i++) {
		scanf("%d", &x[i]);
	}
	int min = x[0], max = x[0];
	for (int i = 0; i < 3; i++) {
		if (x[i] > max) {
			max = x[i];
		}
		if (x[i] < min) {
			min = x[i];
		}
	}
	printf("max = %d\n", max);
	printf("min = %d\n", min);
	return 0;
}
