#include "stdio.h"
int main()
{
	float a, b, c, d, x, x1, x2;
	printf("Введите a, b и c: \n");
	scanf("%f", &a);
	scanf("%f", &b);
	scanf("%f", &c);
	if (a != 0) {
		d = b * b - 4 * a*c;
		if (d >= 0) {
			x1 = (-b + sqrt(d)) / (2 * a);
			x2 = (-b - sqrt(d)) / (2 * a);
			printf("x1 = %f\n", x1);
			printf("x2 = %f\n", x2);
		}
		else
		{
			printf("Комплексные корни\n");
		}
	}
	else if(b!=0)
	{
		x = -c / b;
		printf("x = %f\n", x);
	}
	else if (c!=0)
	{
		printf("Нет решений\n");
	}
	else
	{
		printf("Множество решений\n");
	}
	return 0;
}
